 {\n      return setState({ ...state, error: \"passwords don't match\" });\n    }\n\n    setState({ ...state, error: false });\n\n    signup({ name, email, password }).then((data) => {\n      if (data.error) {\n        setState({ ...state, error: data.error, success: false });\n      } else {\n        setState({\n          ...state,\n          name: \"\",\n          email: \"\",\n          password: \"\",\n          confirmpassword: \"\",\n          error: \"\",\n          success: true,\n        });\n      }\n    });\n  };\n\n  const showError = () => (\n    <div style={{ display: error ? \"\" : \"none\" }}>{error}</div>\n  );\n\n  const showSuccess = () => (\n    <div style={{ display: success ? \"\" : \"none\" }}>\n      SIGNUP SUCCESS, <Link to=\"/signin\">signin here</Link>\n    </div>\n  );\n\n  const redirectUser = () => {\n    if (isAuthenticated()) {\n      return (\n        <Redirect\n          to={\n            isAuthenticated().user.role === 1\n              ? \"/admin/dashboard\"\n              : \"/user/dashboard\"\n          }\n        />\n      );\n    }\n  };\n\n  const signupForm = () => (\n    <div className=\"child\">\n      <form className=\"form\">\n        <Input1 label=\"name\" value={name} onChange={handleChange(\"name\")} />\n        <Input1 label=\"email\" value={email} onChange={handleChange(\"email\")} />\n        <div\n          className=\"show-password-toggle\"\n          onClick={() => setState({ ...state, showPassword: !showPassword })}\n        >\n          show-password\n        </div>\n        <Input1\n          label=\"password\"\n          type={!showPassword ? \"password\" : \"text\"}\n          value={password}\n          onChange={handleChange(\"password\")}\n        />\n        <Input1\n          label=\"confirm\"\n          type={!showPassword ? \"password\" : \"text\"}\n          value={confirmpassword}\n          onChange={handleChange(\"confirmpassword\")}\n        />\n        <button onClick={handleSubmit} className=\"button\">\n          {buttonText}\n        </button>\n      </form>\n      {showError()}\n      {showSuccess()}\n      {redirectUser()}\n    </div>\n  );\n\n  return (\n    <Layout\n      title=\"signup\"\n      description=\"please register\"\n      children={signupForm()}\n    />\n  );\n};\n\nexport default Signup;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\user\\Signin.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\Home.js",["128"],"import { useState, useEffect } from \"react\";\n\nimport Layout from \"./Layout\";\n\nimport { getProducts } from \"./apiCore\";\n\nimport Search from \"./Search\";\n\nimport { Card1 } from \"../utils/cards\";\n\nconst Home = () => {\n  const [productsBySell, setProductsBySell] = useState([]);\n  const [productsByArrival, setProductsByArrival] = useState([]);\n  const [error, setError] = useState(\"\");\n\n  const loadProductsBySell = () => {\n    getProducts(\"sold\").then((data) => {\n      if (data.error) {\n        setError(data.error);\n      } else {\n        setProductsBySell(data);\n      }\n    });\n  };\n\n  const loadProductsByArrival = () => {\n    getProducts(\"createdAt\").then((data) => {\n      if (data.error) {\n        setError(data.error);\n      } else {\n        setProductsByArrival(data);\n      }\n    });\n  };\n\n  useEffect(() => {\n    loadProductsBySell();\n    loadProductsByArrival();\n  }, []);\n\n  const home = (\n    <div className=\"child\">\n      <Search />\n      <h2>new arrivals</h2>\n      <div className=\"card-box\">\n        {productsByArrival.map((product, id) => {\n          return <Card1 key={id} product={product} />;\n        })}\n      </div>\n      <h2>new arrivals</h2>\n      <div className=\"card-box\">\n        {productsBySell.map((product, id) => {\n          return <Card1 key={id} product={product} />;\n        })}\n      </div>\n    </div>\n  );\n\n  return (\n    <Layout\n      title=\"devstones\"\n      description=\"online bookstore for developers and programmers\"\n      children={home}\n    />\n  );\n};\n\nexport default Home;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\Menu.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\Layout.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\config.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\auth\\index.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\user\\UserDashboard.js",["129","130"],"import { Link } from \"react-router-dom\";\n\nimport { isAuthenticated } from \"../auth\";\n\nimport \"./style.min.css\";\n\nimport Layout from \"../core/Layout\";\n\nconst UserDashboard = () => {\n  const {\n    user: { _id, name, email, role },\n  } = isAuthenticated();\n\n  const userLinks = () => {\n    return (\n      <div className=\"dashboard-card\">\n        <h3 className=\"dashboard-card-title\">user links</h3>\n        <ul className=\"dashboard-card-items\">\n          <li className=\"dashboard-card-item\">\n            <Link to=\"/cart\">my cart</Link>\n          </li>\n          <li className=\"dashboard-card-item\">\n            <Link to=\"/profile/update\">update profile</Link>\n          </li>\n        </ul>\n      </div>\n    );\n  };\n\n  const userInfo = () => {\n    return (\n      <div className=\"dashboard-card\">\n        <h3 className=\"dashboard-card-title\">user information</h3>\n        <ul className=\"dashboard-card-items\">\n          <li className=\"dashboard-card-item\">{name}</li>\n          <li className=\"dashboard-card-item\">{email}</li>\n          <li className=\"dashboard-card-item\">user</li>\n        </ul>\n      </div>\n    );\n  };\n\n  const purchaseHistory = () => {\n    return (\n      <div className=\"dashboard-card\">\n        <h3 className=\"dashboard-card-title\">purchase history</h3>\n        <ul className=\"dashboard-card-items\">\n          <li className=\"dashboard-card-item\">history</li>\n        </ul>\n      </div>\n    );\n  };\n\n  const dashboard = () => (\n    <div className=\"child\">\n      <div className=\"dashboard\">\n        <div className=\"dashboard-column\">{userLinks()}</div>\n        <div className=\"dashboard-column\">\n          {userInfo()}\n          {purchaseHistory()}\n        </div>\n      </div>\n    </div>\n  );\n\n  return (\n    <Layout\n      title=\"dashboard\"\n      description={`welcome ${name}!`}\n      children={dashboard()}\n    />\n  );\n};\n\nexport default UserDashboard;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\auth\\PrivateRoute.js",["131"],"import { Component } from \"react\";\n\nimport { Route, Redirect } from \"react-router-dom\";\n\nimport { isAuthenticated } from \"./index\";\n\nconst PrivateRoute = ({ component: Component, ...rest }) => (\n  <Route\n    {...rest}\n    render={(props) =>\n      isAuthenticated() && isAuthenticated().user.role === 0 ? (\n        <Component {...props} />\n      ) : (\n        <Redirect\n          to={{ pathname: \"/signin\", state: { from: props.location } }}\n        />\n      )\n    }\n  />\n);\n\nexport default PrivateRoute;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\user\\AdminDashboard.js",["132","133"],"import { Link } from \"react-router-dom\";\n\nimport { isAuthenticated } from \"../auth\";\n\nimport \"./style.min.css\";\n\nimport Layout from \"../core/Layout\";\n\nconst AdminDashboard = () => {\n  const {\n    user: { _id, name, email, role },\n  } = isAuthenticated();\n\n  const adminLinks = () => {\n    return (\n      <div className=\"dashboard-card\">\n        <h3 className=\"dashboard-card-title\">admin links</h3>\n        <ul className=\"dashboard-card-items\">\n          <li className=\"dashboard-card-item\">\n            <Link to=\"/create/category\">create category</Link>\n          </li>\n          <li className=\"dashboard-card-item\">\n            <Link to=\"/create/product\">create product</Link>\n          </li>\n        </ul>\n      </div>\n    );\n  };\n\n  const adminInfo = () => {\n    return (\n      <div className=\"dashboard-card\">\n        <h3 className=\"dashboard-card-title\">admin information</h3>\n        <ul className=\"dashboard-card-items\">\n          <li className=\"dashboard-card-item\">{name}</li>\n          <li className=\"dashboard-card-item\">{email}</li>\n          <li className=\"dashboard-card-item\">admin</li>\n        </ul>\n      </div>\n    );\n  };\n\n  const dashboard = () => (\n    <div className=\"child\">\n      <div className=\"dashboard\">\n        <div className=\"dashboard-column\">{adminLinks()}</div>\n        <div className=\"dashboard-column\">{adminInfo()}</div>\n      </div>\n    </div>\n  );\n\n  return (\n    <Layout\n      title=\"dashboard\"\n      description={`welcome ${name}!`}\n      children={dashboard()}\n    />\n  );\n};\n\nexport default AdminDashboard;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\auth\\AdminRoute.js",["134"],"import { Component } from \"react\";\n\nimport { Route, Redirect } from \"react-router-dom\";\n\nimport { isAuthenticated } from \"./index\";\n\nconst AdminRoute = ({ component: Component, ...rest }) => (\n  <Route\n    {...rest}\n    render={(props) =>\n      isAuthenticated() && isAuthenticated().user.role === 1 ? (\n        <Component {...props} />\n      ) : (\n        <Redirect\n          to={{ pathname: \"/signin\", state: { from: props.location } }}\n        />\n      )\n    }\n  />\n);\n\nexport default AdminRoute;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\utils\\inputs.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\utils\\buttons.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\admin\\AddCategory.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\admin\\apiAdmin.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\admin\\AddProduct.js",["135"],"import { useState, useEffect } from \"react\";\n\nimport { createProduct, getCategories } from \"./apiAdmin\";\n\nimport { isAuthenticated } from \"../auth\";\n\nimport \"./style.min.css\";\n\nimport Layout from \"../core/Layout\";\n\nimport { Button1 } from \"../utils/buttons\";\nimport { Input1, Input2, Input3, Input4 } from \"../utils/inputs\";\nimport { ReturnLink } from \"../utils/links\";\n\nconst AddProduct = () => {\n  const [state, setState] = useState({\n    name: \"\",\n    description: \"\",\n    price: \"\",\n    categories: [],\n    category: \"\",\n    shipping: \"\",\n    quantity: \"\",\n    photo: \"\",\n    error: \"\",\n    createdProduct: \"\",\n    formData: \"\",\n    loading: false,\n  });\n\n  const {\n    name,\n    description,\n    price,\n    categories,\n    category,\n    shipping,\n    quantity,\n    error,\n    createdProduct,\n    formData,\n    loading,\n  } = state;\n\n  const { user, token } = isAuthenticated();\n\n  const init = () => {\n    getCategories().then((data) => {\n      if (data.error) {\n        setState({ ...state, error: data.error });\n      } else {\n        setState({\n          ...state,\n          categories: data,\n          formData: new FormData(),\n        });\n      }\n    });\n  };\n\n  useEffect(() => {\n    init();\n  }, []);\n\n  const handleChange = (name) => (e) => {\n    const value = name === \"photo\" ? e.target.files[0] : e.target.value;\n\n    formData.set(name, value);\n\n    setState({ ...state, [name]: value });\n  };\n\n  const clickSubmit = (e) => {\n    e.preventDefault();\n\n    setState({ ...state, error: \"\", loading: true });\n\n    createProduct(user._id, token, formData).then((data) => {\n      if (data.error) {\n        setState({ ...state, error: data.error });\n      } else {\n        setState({\n          ...state,\n          name: \"\",\n          description: \"\",\n          photo: \"\",\n          price: \"\",\n          quantity: \"\",\n          loading: false,\n          createdProduct: data.name,\n        });\n      }\n    });\n  };\n\n  const showError = () => {\n    if (error) {\n      return (\n        <h3\n          className=\"alert alert-failure\"\n          style={{ display: error ? \"\" : \"none\" }}\n        >\n          {error}\n        </h3>\n      );\n    }\n  };\n\n  const showSuccess = () => {\n    if (error) {\n      return (\n        <h3\n          className=\"alert alert-success\"\n          style={{ display: createdProduct ? \"\" : \"none\" }}\n        >\n          {`${createdProduct} added`}\n        </h3>\n      );\n    }\n  };\n\n  const showLoading = () =>\n    loading && (\n      <h3\n        className=\"alert alert-success\"\n        style={{ display: createdProduct ? \"\" : \"none\" }}\n      >\n        {`${createdProduct} added`}\n      </h3>\n    );\n\n  const newProductForm = () => (\n    <div className=\"child\">\n      <ReturnLink path=\"/admin/dashboard\" page=\"dashboard\" />\n      <form className=\"form\" onSubmit={clickSubmit}>\n        <Input4 label=\"photo\" onChange={handleChange(\"photo\")} />\n\n        <Input1 label=\"name\" value={name} onChange={handleChange(\"name\")} />\n        <Input2\n          label=\"description\"\n          value={description}\n          onChange={handleChange(\"description\")}\n        />\n        <Input1\n          label=\"price\"\n          type=\"number\"\n          value={price}\n          onChange={handleChange(\"price\")}\n        />\n        {categories && (\n          <Input3\n            label=\"category\"\n            value={category}\n            options={categories}\n            onChange={handleChange(\"category\")}\n          />\n        )}\n        <Input3\n          label=\"shipping\"\n          value={shipping}\n          options={[\n            { _id: 0, name: \"not included\" },\n            { _id: 1, name: \"included\" },\n          ]}\n          onChange={handleChange(\"shipping\")}\n        />\n        <Input1\n          label=\"quantity\"\n          type=\"number\"\n          value={quantity}\n          onChange={handleChange(\"quantity\")}\n        />\n        <Button1 buttonText=\"create product\" />\n      </form>\n      {showError()}\n      {showSuccess()}\n      {showLoading()}\n    </div>\n  );\n\n  return (\n    <Layout\n      title=\"add product\"\n      description=\"please enter a new product\"\n      children={newProductForm()}\n    />\n  );\n};\n\nexport default AddProduct;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\utils\\links.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\apiCore.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\utils\\cards.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\utils\\ShowImage.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\Shop.js",["136","137","138"],"import { useState, useEffect } from \"react\";\n\nimport { getCategories, getFilteredProducts } from \"./apiCore\";\n\nimport Layout from \"./Layout\";\n\nimport CheckBox from \"./CheckBox\";\nimport RadioBox from \"./RadioBox\";\n\nimport { prices } from \"./fixedPrices\";\n\nimport { Card1 } from \"../utils/cards\";\n\nconst Shop = () => {\n  const [myFilters, setMyFilters] = useState({\n    filters: { category: [], price: [] },\n  });\n  const [categories, setCategories] = useState([]);\n  const [error, setError] = useState(false);\n  const [limit, setLimit] = useState(6);\n  const [skip, setSkip] = useState(0);\n  const [size, setSize] = useState(0);\n  const [filteredResults, setFilteredResults] = useState([]);\n\n  const init = () => {\n    getCategories().then((data) => {\n      if (data.error) {\n        setError(data.error);\n      } else {\n        setCategories(data);\n      }\n    });\n  };\n\n  const loadFilteredResults = (newFilters) => {\n    getFilteredProducts(skip, limit, newFilters).then((data) => {\n      if (data.error) {\n        setError(data.error);\n      } else {\n        setFilteredResults(data.data);\n        setSize(data.size);\n        setSkip(0);\n      }\n    });\n  };\n\n  const loadMore = () => {\n    let toSkip = skip + limit;\n\n    getFilteredProducts(toSkip, limit, myFilters.filters).then((data) => {\n      if (data.error) {\n        setError(data.error);\n      } else {\n        setFilteredResults([...filteredResults, ...data.data]);\n        setSize(data.size);\n        setSkip(toSkip);\n      }\n    });\n  };\n\n  const loadMoreButton = () => {\n    return (\n      size > 0 &&\n      size >= limit && (\n        <button className=\"load-more-button\" onClick={loadMore}>\n          load more\n        </button>\n      )\n    );\n  };\n\n  useEffect(() => {\n    init();\n    loadFilteredResults(skip, limit, myFilters.filters);\n  }, []);\n\n  const handleFilters = (filters, filterBy) => {\n    const newFilters = { ...myFilters };\n\n    newFilters.filters[filterBy] = filters;\n\n    if (filterBy === \"price\") {\n      let priceValues = handlePrice(filters);\n\n      newFilters.filters[filterBy] = priceValues;\n    }\n\n    loadFilteredResults(myFilters.filters);\n\n    setMyFilters(newFilters);\n  };\n\n  const handlePrice = (value) => {\n    const data = prices;\n    let array = [];\n\n    for (let key in data) {\n      if (data[key]._id === parseInt(value)) {\n        array = data[key].array;\n      }\n    }\n\n    return array;\n  };\n\n  const shop = (\n    <div className=\"child shop\">\n      <div className=\"shop-filter\">\n        <h4 className=\"shop-filter-heading\">filter by categories</h4>\n        <ul className=\"shop-filter-buttons\">\n          <CheckBox\n            categories={categories}\n            handleFilters={(filters) => handleFilters(filters, \"category\")}\n          />\n        </ul>\n        <h4 className=\"shop-filter-heading\">filter by price</h4>\n        <div className=\"shop-filter-buttons\">\n          <RadioBox\n            prices={prices}\n            handleFilters={(filters) => handleFilters(filters, \"price\")}\n          />\n        </div>\n      </div>\n      <div className=\"shop-items\">\n        <div className=\"shop-cards\">\n          {filteredResults.map((product, i) => (\n            <Card1 key={i} product={product} />\n          ))}\n        </div>\n        {loadMoreButton()}\n      </div>\n    </div>\n  );\n\n  return (\n    <Layout\n      title=\"shop\"\n      description=\"select books by category and price range\"\n      children={shop}\n    />\n  );\n};\n\nexport default Shop;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\CheckBox.js",["139"],"import { useState, useEffect } from \"react\";\n\nconst CheckBox = ({ categories, handleFilters }) => {\n  const [checked, setChecked] = useState([]);\n\n  const handleToggle = (c) => () => {\n    const currentCategoryId = checked.indexOf(c);\n    const newCheckedCategoryId = [...checked];\n    if (currentCategoryId === -1) {\n      newCheckedCategoryId.push(c);\n    } else {\n      newCheckedCategoryId.splice(currentCategoryId, 1);\n    }\n    setChecked(newCheckedCategoryId);\n    handleFilters(newCheckedCategoryId);\n  };\n\n  return categories.map((c, i) => (\n    <li className=\"shop-filter-button\" key={i}>\n      <input\n        onChange={handleToggle(c._id)}\n        value={checked.indexOf(c._id === 1)}\n        type=\"checkbox\"\n      />\n      <label> {c.name}</label>\n    </li>\n  ));\n};\n\nexport default CheckBox;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\fixedPrices.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\RadioBox.js",["140","141","142"],"import { useState, useEffect, Fragment } from \"react\";\n\nconst RadioBox = ({ prices, handleFilters }) => {\n  const [value, setValue] = useState(0);\n\n  const handleChange = (e) => {\n    handleFilters(e.target.value);\n\n    setValue(e.target.value);\n  };\n\n  return prices.map((p, i) => (\n    <div className=\"shop-filter-button\" key={i}>\n      <input onChange={handleChange} value={`${p._id}`} name={p} type=\"radio\" />\n      <label> {p.name}</label>\n    </div>\n  ));\n};\n\nexport default RadioBox;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\Search.js",[],{"ruleId":"143","replacedBy":"144"},{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","severity":1,"message":"148","line":11,"column":10,"nodeType":"149","messageId":"150","endLine":11,"endColumn":17},{"ruleId":"147","severity":1,"message":"151","line":14,"column":10,"nodeType":"149","messageId":"150","endLine":14,"endColumn":15},{"ruleId":"147","severity":1,"message":"152","line":11,"column":13,"nodeType":"149","messageId":"150","endLine":11,"endColumn":16},{"ruleId":"147","severity":1,"message":"153","line":11,"column":31,"nodeType":"149","messageId":"150","endLine":11,"endColumn":35},{"ruleId":"147","severity":1,"message":"154","line":1,"column":10,"nodeType":"149","messageId":"150","endLine":1,"endColumn":19},{"ruleId":"147","severity":1,"message":"152","line":11,"column":13,"nodeType":"149","messageId":"150","endLine":11,"endColumn":16},{"ruleId":"147","severity":1,"message":"153","line":11,"column":31,"nodeType":"149","messageId":"150","endLine":11,"endColumn":35},{"ruleId":"147","severity":1,"message":"154","line":1,"column":10,"nodeType":"149","messageId":"150","endLine":1,"endColumn":19},{"ruleId":"155","severity":1,"message":"156","line":63,"column":6,"nodeType":"157","endLine":63,"endColumn":8,"suggestions":"158"},{"ruleId":"147","severity":1,"message":"151","line":19,"column":10,"nodeType":"149","messageId":"150","endLine":19,"endColumn":15},{"ruleId":"147","severity":1,"message":"159","line":20,"column":17,"nodeType":"149","messageId"