atch((err) => {\n            console.log(\"ERROR: \" + err);\n            setData({ loading: false });\n          });\n      })\n      .catch((error) => {\n        // console.log(\"dropin error: \", error);\n        setData({ ...data, error: error.message });\n      });\n  };\n\n  const showError = (error) => (\n    <div style={{ display: error ? \"\" : \"none\" }}>{error}</div>\n  );\n\n  const showSuccess = (success) => (\n    <div style={{ display: success ? \"\" : \"none\" }}>payment successful</div>\n  );\n\n  return (\n    <div>\n      <div>total: Â£{getTotal()}</div> :\n      {isAuthenticated() ? (\n        <div onBlur={() => setData({ ...data, error: \"\" })}>\n          {showSuccess(data.success)}\n          {showError(data.error)}\n          {data.loading && <div>LOADING...</div>}\n          {data.redirect && <Redirect to=\"/\" />}\n          {data.clientToken !== null && products.length > 0 ? (\n            <div>\n              <DropIn\n                options={{\n                  authorization: data.clientToken,\n                  paypal: { flow: \"vault\" },\n                }}\n                onInstance={(instance) => (data.instance = instance)}\n              />\n              <button onClick={buy}>pay</button>\n            </div>\n          ) : null}\n        </div>\n      ) : (\n        <Link to=\"/signin\">sign in to check out</Link>\n      )}\n    </div>\n  );\n};\n\nexport default Checkout;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\CartItem.js",["169"],{"ruleId":"170","replacedBy":"171"},{"ruleId":"172","replacedBy":"173"},{"ruleId":"174","severity":1,"message":"175","line":40,"column":6,"nodeType":"176","endLine":40,"endColumn":8,"suggestions":"177"},{"ruleId":"178","severity":1,"message":"179","line":11,"column":13,"nodeType":"180","messageId":"181","endLine":11,"endColumn":16},{"ruleId":"178","severity":1,"message":"182","line":11,"column":31,"nodeType":"180","messageId":"181","endLine":11,"endColumn