Password,\n    buttonText,\n  } = state;\n\n  const handleChange = (name) => (e) => {\n    setState({ ...state, error: \"\", [name]: e.target.value });\n  };\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n\n    if (password !== confirmpassword) {\n      return setState({ ...state, error: \"passwords don't match\" });\n    }\n\n    setState({ ...state, error: false });\n\n    signup({ name, email, password }).then((data) => {\n      if (data.error) {\n        setState({ ...state, error: data.error, success: false });\n      } else {\n        setState({\n          ...state,\n          name: \"\",\n          email: \"\",\n          password: \"\",\n          confirmpassword: \"\",\n          error: \"\",\n          success: true,\n        });\n      }\n    });\n  };\n\n  const showError = () => (\n    <div style={{ display: error ? \"\" : \"none\" }}>{error}</div>\n  );\n\n  const showSuccess = () => (\n    <div style={{ display: success ? \"\" : \"none\" }}>\n      SIGNUP SUCCESS, <Link to=\"/signin\">signin here</Link>\n    </div>\n  );\n\n  const redirectUser = () => {\n    if (isAuthenticated()) {\n      return (\n        <Redirect\n          to={\n            isAuthenticated().user.role === 1\n              ? \"/admin/dashboard\"\n              : \"/user/dashboard\"\n          }\n        />\n      );\n    }\n  };\n\n  const signupForm = () => (\n    <div className=\"child\">\n      <form className=\"form\">\n        <Input1 label=\"name\" value={name} onChange={handleChange(\"name\")} />\n        <Input1 label=\"email\" value={email} onChange={handleChange(\"email\")} />\n        <div\n          className=\"show-password-toggle\"\n          onClick={() => setState({ ...state, showPassword: !showPassword })}\n        >\n          show-password\n        </div>\n        <Input1\n          label=\"password\"\n          type={!showPassword ? \"password\" : \"text\"}\n          value={password}\n          onChange={handleChange(\"password\")}\n        />\n        <Input1\n          label=\"confirm\"\n          type={!showPassword ? \"password\" : \"text\"}\n          value={confirmpassword}\n          onChange={handleChange(\"confirmpassword\")}\n        />\n        <button onClick={handleSubmit} className=\"button\">\n          {buttonText}\n        </button>\n      </form>\n      {showError()}\n      {showSuccess()}\n      {redirectUser()}\n    </div>\n  );\n\n  return (\n    <Layout\n      title=\"signup\"\n      description=\"please register\"\n      children={signupForm()}\n    />\n  );\n};\n\nexport default Signup;\n","C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\user\\Signin.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\Home.js",["103"],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\Menu.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\core\\Layout.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\config.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\auth\\index.js",[],"C:\\Users\\dougi\\OneDrive\\Desktop\\files\\MERN\\devstones\\client\\src\\user\\UserDashboard.js",["104","105"],"import { Link } from \"react-router-dom\";\n\nimport { isAuthenticated } from \"../auth\";\n\nimport \"./style.min.css\";\n\nimport Layout from \"../core/Layout\";\n\nconst UserDashboard = () => {\n  const {\n    user: { _id, name, email, role },\n  } = isAuthenticated();\n\n  const userLinks = () => {\n    return (\n      <div className=\"dashboard-card\">\n        <h3 className=\"dashboard-card-title\">user links</h3>\n        <ul className=\"dashboard-card-items\">\n          <li className=\"dashboard-card-item\">\n            <Link to=\"/cart\">my cart</Link>\n          </li>\n          <li className=\"dashboard-card-item\">\n            <Link to=\"/profile/update\">update profile</Link>\n          </li>\n        </ul>\n      </